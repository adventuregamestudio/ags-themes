{
  "name": "VS2015 Blue Theme",
  "version": "0.1.0",
  "background": "#293955",
  "global": {
    "listview": {
      "background": "#fff",
      "foreground": "#000",
      "owner-draw": true,
      "grid-lines": false,
      "last-column-width": -2,
      "draw-item": true,
      "draw-sub-item": true,
      "column-header": {
        "background": "#f6f6f6",
        "foreground": "#000",
        "border": "#cccedb"
      }
    },
    "propertygrid": {
      "background": "#cfd6e5",
      "line": "#eeeef2",
      "category": "#000",
      "view": {
        "background": "#fff",
        "foreground": "#000",
        "border": "#eeeef2"
      },
      "help": {
        "background": "#fff",
        "foreground": "#000",
        "border": "#fff"
      }
    }
  },
  "main-container": {
    "dock-background": "transparent",
    "background": "#293955",
    "foreground": "#000",
    "skin": {
      "auto-hide": {
        "tab-gradient": {
          "start": "#293955",
          "end": "#293955",
          "text": "#fff"
        },
        "dock-strip-gradient": {
          "start": "#293955",
          "end": "#293955"
        }
      },
      "dock-pane": {
        "document-gradient": {
          "dock-strip-gradient": {
            "start": "#293955",
            "end": "#293955"
          },
          "active-tab-gradient": {
            "start": "#fff29d",
            "end": "#fff29d",
            "text": "#000"
          },
          "inactive-tab-gradient": {
            "start": "#364e6f",
            "end": "#364e6f",
            "text": "#fff"
          }
        },
        "tool-window": {
          "active-caption-gradient": {
            "start": "#fff29d",
            "end": "#fff29d",
            "text": "#000"
          },
          "inactive-caption-gradient": {
            "start": "#4d6082",
            "end": "#4d6082",
            "text": "#fff"
          },
          "active-tab-gradient": {
            "start": "#fff",
            "end": "#fff",
            "text": "#000"
          },
          "inactive-tab-gradient": {
            "start": "#4b5c74",
            "end": "#4b5c74",
            "text": "#fff"
          },
          "dock-strip-gradient": {
            "start": "#293955",
            "end": "#293955"
          }
        }
      }
    }
  },
  "main-menu": {
    "background": "#d6dbe9",
    "foreground": "#000",
    "border": "#9ba7b7",
    "background-dropdown": "#eaf0ff",
    "separator": "#bec3cb",
    "selected": {
      "gradient": {
        "begin": "#fdf4bf",
        "end": "#fdf4bf"
      }
    },
    "pressed": {
      "gradient": {
        "begin": "#eaf0ff",
        "middle": "#eaf0ff",
        "end": "#eaf0ff"
      }
    },
    "item": {
      "border": "#d6dbe9",
      "selected": "#fdf4bf"
    },
    "check": {
      "background": "#e9ecee",
      "foreground": "#000",
      "border": "#e9ecee",
      "selected": "#fdf4bf",
      "pressed": "#fdf4bf"
    },
    "margin": {
      "gradient": {
        "begin": "#f2f4fe",
        "middle": "#f2f4fe",
        "end": "#f2f4fe"
      }
    }
  },
  "tool-bar": {
    "background": "#d6dbe9",
    "border": "#dce0ec",
    "separator": "#222222",
    "selected-gradient": {
      "begin": "#fdf4bf",
      "end": "#fdf4bf"
    },
    "overflow-gradient": {
      "begin": "#fdf4bf",
      "middle": "#fdf4bf",
      "end": "#fdf4bf"
    },
    "gradient": {
      "begin": "#cfd6e5",
      "middle": "#cfd6e5",
      "end": "#cfd6e5"
    },
    "grip": {
      "light": "#cfd6e5",
      "dark": "#60728c"
    }
  },
  "status-strip": {
    "background": "#007acc",
    "foreground": "#ffffff"
  },
  "welcome": {
    "background": "#fff",
    "foreground": "#000",
    "panel1": {
      "background": "#4169e1",
      "foreground": "#ffffff"
    },
    "panel2": {
      "background": "#efeff2",
      "foreground": "#000",
      "link": "#007acc"
    },
    "pnlTipOfTheDay": {
      "background": "#efeff2",
      "foreground": "#000",
      "link": "#007acc"
    },
    "pnlRight": {
      "background": "#fff",
      "foreground": "#000",
      "link": "#007acc"
    }
  },
  "script-editor": {
    "background": "#d6dbe9",
    "foreground": "#c8c8c8",
    "text-editor": {
      "global-default": {
        "background": "#fff",
        "foreground": "#000"
      },
      "default": {
        "background": "#fff",
        "foreground": "#000"
      },
      "word-1": {
        "background": "#fff",
        "foreground": "#0000ff"
      },
      "word-2": {
        "background": "#fff",
        "foreground": "#2b91af"
      },
      "identifier": {
        "background": "#fff",
        "foreground": "#000"
      },
      "comment": {
        "background": "#fff",
        "foreground": "#008000"
      },
      "comment-line": {
        "background": "#fff",
        "foreground": "#008000"
      },
      "comment-doc": {
        "background": "#fff",
        "foreground": "#006400"
      },
      "comment-line-doc": {
        "background": "#fff",
        "foreground": "#006400"
      },
      "comment-doc-keyword": {
        "background": "#fff",
        "foreground": "#006400"
      },
      "comment-doc-keyword-error": {
        "background": "#fff",
        "foreground": "#99a38a"
      },
      "number": {
        "background": "#fff",
        "foreground": "#000"
      },
      "regex": {
        "background": "#fff",
        "foreground": "#e68d07"
      },
      "string": {
        "background": "#fff",
        "foreground": "#a31515"
      },
      "string-eol": {
        "background": "#fff",
        "foreground": "#c15555"
      },
      "operator": {
        "background": "#fff",
        "foreground": "#000"
      },
      "preprocessor": {
        "background": "#fff",
        "foreground": "#808080"
      },
      "line-number": {
        "background": "#ffffff",
        "foreground": "#2b91af"
      },
      "indent-guide": {
        "background": "#fff",
        "foreground": "#2b91af"
      },
      "fold-margin": "#fff",
      "fold-margin-hi": "#fff",
      "marknum-folder": {
        "background": "#a5a5a5",
        "foreground": "#e2e2e2"
      },
      "marknum-folder-end": {
        "background": "#a5a5a5",
        "foreground": "#e2e2e2"
      },
      "marknum-folder-open": {
        "background": "#a5a5a5",
        "foreground": "#fff"
      },
      "marknum-folder-open-mid": {
        "background": "#a5a5a5",
        "foreground": "#fff"
      },
      "marknum-folder-mid-tail": "#a5a5a5",
      "marknum-folder-sub": "#a5a5a5",
      "marknum-folder-tail": "#a5a5a5",
      "selected": {
        "background": "#99c9ef",
        "foreground": "transparent"
      },
      "caret": "#000",
      "marker-breakpoint": {
        "background": "#f596a4",
        "foreground": "#ffffff"
      },
      "marker-breakpoint2": {
        "background": "#d42b2c",
        "foreground": "#f7d5d5"
      },
      "current-statement": {
        "background": "#fff181",
        "foreground": "#000"
      },
      "current-statement2": {
        "background": "#fff1af",
        "foreground": "transparent"
      }
    }
  }
}