{
  "name": "Blue Theme plus Obsidian",
  "version": "0.1.1",
  "background": "#293955",
  "global": {
    "listview": {
      "background": "#fff",
      "foreground": "#000",
      "owner-draw": true,
      "grid-lines": false,
      "last-column-width": -2,
      "draw-item": true,
      "draw-sub-item": true,
      "column-header": {
        "background": "#f6f6f6",
        "foreground": "#000",
        "border": "#cccedb"
      }
    },
    "propertygrid": {
      "background": "#cfd6e5",
      "line": "#eeeef2",
      "category": "#000",
      "view": {
        "background": "#fff",
        "foreground": "#000",
        "border": "#eeeef2"
      },
      "help": {
        "background": "#fff",
        "foreground": "#000",
        "border": "#fff"
      }
    }
  },
  "main-container": {
    "dock-background": "transparent",
    "background": "#293955",
    "foreground": "#000",
    "skin": {
      "auto-hide": {
        "tab-gradient": {
          "start": "#293955",
          "end": "#293955",
          "text": "#fff"
        },
        "dock-strip-gradient": {
          "start": "#293955",
          "end": "#293955"
        }
      },
      "dock-pane": {
        "document-gradient": {
          "dock-strip-gradient": {
            "start": "#293955",
            "end": "#293955"
          },
          "active-tab-gradient": {
            "start": "#fff29d",
            "end": "#fff29d",
            "text": "#000"
          },
          "inactive-tab-gradient": {
            "start": "#364e6f",
            "end": "#364e6f",
            "text": "#fff"
          }
        },
        "tool-window": {
          "active-caption-gradient": {
            "start": "#fff29d",
            "end": "#fff29d",
            "text": "#000"
          },
          "inactive-caption-gradient": {
            "start": "#4d6082",
            "end": "#4d6082",
            "text": "#fff"
          },
          "active-tab-gradient": {
            "start": "#fff",
            "end": "#fff",
            "text": "#000"
          },
          "inactive-tab-gradient": {
            "start": "#4b5c74",
            "end": "#4b5c74",
            "text": "#fff"
          },
          "dock-strip-gradient": {
            "start": "#293955",
            "end": "#293955"
          }
        }
      }
    }
  },
  "main-menu": {
    "background": "#d6dbe9",
    "foreground": "#000",
    "border": "#9ba7b7",
    "background-dropdown": "#eaf0ff",
    "separator": "#bec3cb",
    "selected": {
      "gradient": {
        "begin": "#fdf4bf",
        "end": "#fdf4bf"
      }
    },
    "pressed": {
      "gradient": {
        "begin": "#eaf0ff",
        "middle": "#eaf0ff",
        "end": "#eaf0ff"
      }
    },
    "item": {
      "border": "#d6dbe9",
      "selected": "#fdf4bf"
    },
    "check": {
      "background": "#e9ecee",
      "foreground": "#000",
      "border": "#e9ecee",
      "selected": "#fdf4bf",
      "pressed": "#fdf4bf"
    },
    "margin": {
      "gradient": {
        "begin": "#f2f4fe",
        "middle": "#f2f4fe",
        "end": "#f2f4fe"
      }
    }
  },
  "tool-bar": {
    "background": "#d6dbe9",
    "border": "#dce0ec",
    "separator": "#222222",
    "selected-gradient": {
      "begin": "#fdf4bf",
      "end": "#fdf4bf"
    },
    "overflow-gradient": {
      "begin": "#fdf4bf",
      "middle": "#fdf4bf",
      "end": "#fdf4bf"
    },
    "gradient": {
      "begin": "#cfd6e5",
      "middle": "#cfd6e5",
      "end": "#cfd6e5"
